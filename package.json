{
  "name": "radiodan-magic-button",
  "version": "0.0.0",
  "description": "The radiodan demo app",
  "main": "main.js",
  "scripts": {
    "postinstall": "cp -n config/radiodan-config.json.example config/radiodan-config.json; cp -n config/physical-ui-config.json.example config/physical-ui-config.json; exit 0",
    "test": "LOG_LEVEL=fatal node_modules/mocha/bin/mocha --recursive --require test/helper",
    "coverage": "LOG_LEVEL=fatal node_modules/istanbul/lib/cli.js cover _mocha -- --require test/helper --recursive --ui bdd -R spec -t 5000",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "dev": "NODE_ENV=development nodemon main.js",
    "start": "node main.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/radiodan/magic-button.git"
  },
  "keywords": [
    "radiodan"
  ],
  "author": "Dan Nuttall <dan.nuttall@bbc.co.uk>",
  "license": "Apache-2",
  "bugs": {
    "url": "https://github.com/radiodan/magic-button/issues"
  },
  "homepage": "https://github.com/radiodan/magic-button",
  "dependencies": {
    "express": "^4.0.0",
    "radiodan-client": "https://github.com/radiodan/radiodan-client.js/archive/master.tar.gz",
    "eventsource": "^0.1",
    "method-override": "^1.0.0",
    "body-parser": "^1.0.0",
    "errorhandler": "^1.0.0",
    "serve-static": "^1.0.2",
    "morgan": "^1.0.0",
    "nedb": "^0.10.4",
    "swig": "^1.3.2",
    "request": "~2.34.0",
    "express-eventsource": "0.0.3",
    "deepequal": "0.0.1",
    "machina": "^0.3.6"
  },
  "devDependencies": {
    "backbone": "~1.1.2",
    "chai": "^1.9.0",
    "chai-as-promised": "^4.1.1",
    "es6-promise": "^0.1.1",
    "grunt": "~0.4.5",
    "grunt-autoprefixer": "~0.7.2",
    "grunt-browserify": "~2.0.1",
    "grunt-contrib-copy": "~0.5.0",
    "grunt-contrib-less": "~0.11.0",
    "grunt-contrib-uglify": "~0.4.0",
    "grunt-contrib-watch": "~0.5.3",
    "grunt-mocha-test": "^0.12.1",
    "istanbul": "^0.2.6",
    "jquery": "~2.1.0",
    "mocha": "^1.21.5",
    "nodemon": "^1.0.15",
    "ractive": "~0.5.5",
    "ractive-backbone": "0.1.0",
    "sinon": "^1.9.0",
    "supertest": "^0.9.0",
    "underscore": "~1.6.0"
  }
}
